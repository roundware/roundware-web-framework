{
  "name": "roundware-web-framework",
  "homepage": "https://roundware.github.io/roundware-web-framework/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roundware/roundware-web-framework.git"
  },
  "version": "0.11.0-alpha.30",
  "description": "Client framework for Roundware, a location-based contributory audio platform",
  "main": "dist/roundware.js",
  "files": [
    "dist"
  ],
  "types": "dist/roundware.d.ts",
  "scripts": {
    "test": "jest",
    "build": "NODE_ENV=production npm-run-all --parallel build:*",
    "build:main": "babel src --out-dir dist --extensions '.ts' --copy-files src/types/*.d.ts && tsc --declaration --outDir dist/ --emitDeclarationOnly",
    "build:umd": "webpack --mode=production --output-filename roundware.umd.js",
    "build:umd.min": "webpack --mode=production --output-filename roundware.umd.min.js",
    "docbuild": "jsdoc --verbose -c ./config/jsdoc.json",
    "devstart": "NODE_ENV=development webpack server",
    "deploy": "np",
    "lint": "eslint src/"
  },
  "watch": {
    "test": "{src,test}/*.js"
  },
  "keywords": [
    "audio",
    "roundware"
  ],
  "author": "Mike Subelsky <mike@subelsky.com> (http://www.subelsky.com/)",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/roundware/roundware-web-framework/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.14.5",
    "@types/howler": "^2.2.3",
    "@types/jest": "^27.0.1",
    "babel-loader": "^8.2.2",
    "eslint": "6.1.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-compat": "3.3.0",
    "jest": "^27.0.6",
    "jsdoc": "^3.6.6",
    "np": "^6.3.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "~2.6.3",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@turf/boolean-point-in-polygon": "^6.2.0",
    "@turf/distance": "^6.2.0",
    "@turf/line-to-polygon": "^6.2.0",
    "@turf/point-to-line-distance": "^6.2.0",
    "core-js": "^3.17.3",
    "howler": "^2.2.3",
    "loglevel": "^1.7.1",
    "standardized-audio-context": "^25.3.9"
  },
  "np": {
    "anyBranch": true,
    "tests": false,
    "yarn": false
  }
}
